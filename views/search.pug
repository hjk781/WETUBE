extends layouts/main
include mixins/videoBlock

block content
    .search_header
        h3 Search for: #{searchingBy}
    .search_videos
        if videos.length === 0
            h5 No videos Found
        each item in videos
            +videoBlock({
                title:item.title,
                views:item.views,
                videoFile:item.videoFile,
                id:item.id
            })